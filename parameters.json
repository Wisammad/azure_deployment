{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "acrName": {
        "value": "yourAcrName"
      },
      "location": {
        "value": "yourLocation"
      },
      "appServicePlanName": {
        "value": "yourAppServicePlanName"
      },
      "webAppName": {
        "value": "yourWebAppName"
      },
      "containerRegistryName": {
        "value": "yourContainerRegistryName"
      },
      "containerRegistryImageName": {
        "value": "yourContainerRegistryImageName"
      },
      "containerRegistryImageVersion": {
        "value": "yourContainerRegistryImageVersion"
      },
      "dockerRegistryUrl": {
        "value": "https://yourAcrName.azurecr.io"
      },
      "dockerRegistryUsername": {
        "value": "yourDockerRegistryUsername"
      },
      "dockerRegistryPassword": {
        "value": "yourDockerRegistryPassword"
      }
    }
  }
  